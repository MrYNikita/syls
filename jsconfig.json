{
    "include": [
        "./**/*",
        "./-submodule/**/*"
    ],
    "typeAcquisition": {
        "include": [
            "node"
        ],
    },
    "compilerOptions": {
        "module": "ES2022",
        "target": "ES2022",
        "esModuleInterop": true,
        "moduleResolution": "nodenext",
        "resolveJsonModule": true,
        "experimentalDecorators": true,
    }
}